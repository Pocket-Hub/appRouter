{
  "name": "@sap/ams",
  "version": "1.18.0",
  "description": "AMS client library for node.js",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "config": {
    "dcl_target": "dcl-target"
  },
  "scripts": {
    "compile-dcl": "npx compile-dcl -d test/integration/dcl-resources -o $npm_package_config_dcl_target && cp test/integration/dcl-resources/data.json $npm_package_config_dcl_target/data.json",
    "jest": "AMS_DCN_ROOT=$npm_package_config_dcl_target jest",
    "lint": "npx eslint",
    "prepare": "npm i --prefix src/xcode/dcn-engine && npm run build --prefix src/xcode/dcn-engine",
    "pretest": "npm run prepare && npm run compile-dcl",
    "test": "npm run jest",
    "posttest": "rm -rf $npm_package_config_dcl_target"
  },
  "author": "",
  "license": "SAP DEVELOPER LICENSE AGREEMENT",
  "files": [
    "cds-plugin.js",
    "src/xcode/dcn-engine/build",
    "doc",
    "lib",
    "lib/errors",
    "lib/express",
    "package.json",
    "README.md"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@sap/xsenv": "^5.2.0",
    "axios": "^1.6.5",
    "compressing": "^1.9.0",
    "json-to-pretty-yaml": "^1.2.2",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.2.0",
    "@sap/ams-dev": "^1",
    "@sap/audit-logging": "^6.0.0",
    "@sap/cds": "^8.1.0",
    "chai": "^5.0.0",
    "eslint": "^9.2.0",
    "eslint-plugin-unused-imports": "^4.1.3",
    "globals": "^15.2.0",
    "jest": "^29.6.2",
    "rewire": "^7.0.0",
    "uuid": "^10.0.0"
  },
  "peerDependencies": {
    "@sap/cds": ">=7.5"
  },
  "peerDependenciesMeta": {
    "@sap/cds": {
      "optional": true
    }
  },
  "cds": {
    "requires": {
      "kinds": {
        "AmsEventService": {
          "impl": "@sap/ams/lib/cap/AmsEventService"
        }
      },
      "auth": {
        "ams": {
          "dclRoot": "gen/srv/ams/dcl",
          "customDclRoot": "ams/dcl",
          "policyDeployerRoot": "gen/policies"
        },
        "[java]": {
          "ams": {
            "dclRoot": "srv/src/main/resources/ams",
            "customDclRoot": "srv/src/main/resources/ams",
            "policyDeployerRoot": "srv/src/gen/policies"
          }
        }
      }
    }
  }
}